{% extends "GitonomyFrontendBundle:BaseAdmin:delete.html.twig" %}

{% block title 'Delete '~object %}
{% set navigation_active = null %}

{% block content %}
    <div class="page-header">
        <h1>Delete "{{ object }}" ?</h1>
    </div>
    <form class="form-stacked" action="{{ path('gitonomyfrontend_email_profile_delete', {'id':object.id}) }}" id="delete" method="POST" novalidate>
        {{ form_errors(form) }}
        {{ form_rest(form) }}

        <input type="submit" class="btn danger" value="Delete" />
        <a href="{{ path('gitonomyfrontend_profile_index') }}" class="btn">Cancel</a>
    </form>
{% endblock %}
