{% extends "GitonomyFrontendBundle:AdminProject:layout.html.twig" %}

{% block title 'Delete Git access' %}
{% set navigation_active = 'gitAccesses' %}

{% block content_right %}
    <div class="page-header">
        <h2>Remove a git access from project {{ object.name }} ?</h2>
    </div>
    <form class="form-stacked" action="{{ path('gitonomyfrontend_adminproject_gitAccessDelete', {'gitAccessId': gitAccess.id}) }}" method="POST" novalidate>
        {{ form_errors(form) }}
        {{ form_rest(form) }}
        <input type="submit" class="btn danger" value="Delete" />
        <a href="{{ path('gitonomyfrontend_adminproject_gitAccesses', {'id':object.id}) }}" class="btn">Cancel</a>
    </form>
{% endblock %}
